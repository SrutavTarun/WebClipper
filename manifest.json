{
    "name": "WebClipper",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "icons":{
        
    },
    "permissions": ["contextMenus","storage", "activeTab","tabs","scripting","webRequest"],
    "background":{
        "service_worker" : "background.js"
    },
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["your/content_main.js"]
      }]
}
